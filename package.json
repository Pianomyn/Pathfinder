{
  "homepage": "https://Pianomyn.github.io/pathfinder",
  "name": "pathfinder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "tailwind-watch": "npx tailwindcss -i ./src/input.css -o ./dist/output.css -w",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist && npm run tailwind",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "gh-pages": "^4.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
